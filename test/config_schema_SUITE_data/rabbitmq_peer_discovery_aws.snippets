                      "test/config_schema_SUITE_data/certs/cacert.pem"},
                  {certfile,"test/config_schema_SUITE_data/certs/cert.pem"},
                  {keyfile,
                      "test/config_schema_SUITE_data/certs/key.pem"}]}]}]}],
  [rabbitmq_management]},
 {retention_policies,
  "management.sample_retention_policies.global.minute  = 5
